<!DOCTYPE aesl-source>
<network>
<event name="pair_run" size="1"/>
<event name="debug_log" size="14"/>
<node nodeId="{3a62a583-b3d2-4dbb-8734-cdda994ea5cf}" name="USB Serial Device"># variables for state
var state[4] = [0,0,0,0]
var new_state[4] = [0,0,0,0]

# variables for notes
var notes[6]
var durations[6]
var note_index = 6
var note_count = 6
var wave[142]
var i
var wave_phase
var wave_intensity

# compute a sinus wave for sound
for i in 0:141 do
	wave_phase = (i-70)*468
	call math.cos(wave_intensity, wave_phase)
	wave[i] = wave_intensity/256
end
call sound.wave(wave)
# stop timer 0
timer.period[0] = 0
# reset outputs
call sound.system(-1)
call leds.top(0,0,0)
call leds.bottom.left(0,0,0)
call leds.bottom.right(0,0,0)
call leds.circle(0,0,0,0,0,0,0,0)

# subroutine to display the current state
sub display_state
	call leds.circle(0,state[1]*32,0,state[3]*32,0,state[2]*32,0,state[0]*32)

# when a note is finished, play the next note
onevent sound.finished
	if note_index != note_count then
		call sound.freq(notes[note_index], durations[note_index])
		note_index += 1
	end

onevent buttons
	when button.forward == 1 do
		motor.left.target = 150
		motor.right.target = 150
		new_state[0] = 1
		new_state[1] = 1
		new_state[2] = 0
		new_state[3] = 0
		timer.period[0] = 1000
		emit pair_run 0
		_emit debug_log [0, 0x5, 0xd871, 0xc0, 0, 0x10, 0, 0xd0d, 0x2c, 0x3e8]
	end

	call math.copy(state, new_state)
	callsub display_state


onevent prox
	when prox.horizontal[0] &lt;= 800 and prox.horizontal[1] &lt;= 800 and prox.horizontal[2] &lt;= 800 do
		if state[0] == 1 and state[1] == 0 and state[2] == 0 and state[3] == 0 then
			motor.left.target = 150
			motor.right.target = 200
			emit pair_run 2
			_emit debug_log [0x2, 0x3, 0x872, 0x2a00, 0x320, 0x7d0, 0x35, 0xd0e]
		end
	end

	when prox.horizontal[0] >= 2400 and prox.horizontal[1] >= 2400 and prox.horizontal[2] >= 2400 do
		if state[0] == 1 and state[1] == 0 and state[2] == 0 and state[3] == 0 then
			motor.left.target = 300
			motor.right.target = 50
			emit pair_run 3
			_emit debug_log [0x3, 0x3, 0x872, 0x1500, 0x2bc, 0x960, 0x35, 0x100b]
		end
	end

	when prox.horizontal[1] >= 2400 do
		if state[0] == 1 and state[1] == 0 and state[2] == 0 then
			motor.left.target = 150
			motor.right.target = 0
			emit pair_run 4
			_emit debug_log [0x4, 0x3, 0x872, 0x400, 0x3e8, 0x960, 0x33, 0xd0a]
		end
	end

	when prox.horizontal[0] >= 2400 do
		if state[0] == 1 and state[1] == 0 and state[2] == 0 and state[3] == 0 then
			motor.left.target = 250
			motor.right.target = 0
			emit pair_run 5
			_emit debug_log [0x5, 0x3, 0x872, 0x1000, 0x3e8, 0x960, 0x35, 0xf0a]
		end
	end

	when prox.horizontal[4] >= 2600 do
		if state[0] == 1 and state[1] == 0 and state[2] == 0 and state[3] == 0 then
			motor.left.target = 100
			motor.right.target = 250
			new_state[0] = 0
			new_state[1] = 0
			new_state[2] = 0
			new_state[3] = 1
			emit pair_run 6
			_emit debug_log [0x6, 0x4, 0xd872, 0x10, 0x3e8, 0xa28, 0x35, 0xc0f, 0x4f]
		end
	end

	when prox.horizontal[0] >= 3200 do
		if state[0] == 0 and state[1] == 0 and state[2] == 0 and state[3] == 1 then
			motor.left.target = 150
			motor.right.target = 100
			emit pair_run 7
			_emit debug_log [0x7, 0x3, 0x872, 0x1000, 0x3e8, 0xc80, 0x4f, 0xd0c]
		end
	end

	when prox.horizontal[4] >= 2400 do
		if state[0] == 0 and state[1] == 0 and state[2] == 0 and state[3] == 1 then
			motor.left.target = 50
			motor.right.target = 350
			emit pair_run 8
			_emit debug_log [0x8, 0x3, 0x872, 0x10, 0x3e8, 0x960, 0x4f, 0xb11]
		end
	end

	when prox.horizontal[2] >= 1400 do
		if state[0] == 0 and state[1] == 0 and state[2] == 0 and state[3] == 1 then
			motor.left.target = -50
			motor.right.target = -50
			timer.period[0] = 1000
			emit pair_run 9
			_emit debug_log [0x9, 0x4, 0xc872, 0x100, 0x3e8, 0x578, 0x4f, 0x909, 0x3e8]
		end
	end

	call math.copy(state, new_state)
	callsub display_state


onevent timer0
	timer.period[0] = 0
	if state[0] == 1 and state[1] == 1 and state[2] == 0 and state[3] == 0 then
		motor.left.target = 250
		motor.right.target = 250
		new_state[0] = 1
		new_state[1] = 0
		new_state[2] = 0
		new_state[3] = 0
		emit pair_run 1
		_emit debug_log [0x1, 0x4, 0xd876, 0x2c, 0xf0f, 0x35]
	end

	if state[0] == 0 and state[1] == 0 and state[2] == 0 and state[3] == 1 then
		motor.left.target = 0
		motor.right.target = 0
		call math.copy(notes[0:5], [440, 524, 440, 370, 311, 370])
		call math.copy(durations[0:5], [7, 7, 14, 7, 7, 14])
		note_index = 1
		note_count = 6
		call sound.freq(notes[0], durations[0])
		call leds.top(32,0,3)
		emit pair_run 10
		_emit debug_log [0xa, 0x5, 0xb876, 0x90, 0x4f, 0xa0a, 0x7238, 0x188, 0x8823]
	end

	call math.copy(state, new_state)
	callsub display_state

